{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "strict": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "skipLibCheck": true
	},
	"include": ["server/src/**/*"],
	"exclude": ["node_modules", "dist", "client"]
  }
  